<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_list">

    <fragment
        android:id="@+id/nav_edit"
        android:name="edu.rosehulman.photobucket.ui.PhotoEditFragment"
        android:label="Edit"
        tools:layout="@layout/fragment_photo_edit" />

    <fragment
        android:id="@+id/nav_list"
        android:name="edu.rosehulman.photobucket.ui.PhotosListFragment"
        android:label="Photos"
        tools:layout="@layout/fragment_photos_list" />

    <fragment
        android:id="@+id/nav_detail"
        android:name="edu.rosehulman.photobucket.ui.PhotoDetailFragment"
        android:label="Detail"
        tools:layout="@layout/fragment_photo_detail" />

    <fragment
        android:id="@+id/nav_user"
        android:name="edu.rosehulman.photobucket.ui.user.UserFragment"
        android:label="@string/menu_user"
        tools:layout="@layout/fragment_user" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="edu.rosehulman.photobucket.ui.settings.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings" />
</navigation>